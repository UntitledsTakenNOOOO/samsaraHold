        TNT1 A 0 A_GiveInventory("RocketAmmo",5)
        TNT1 A 0 A_GiveInventory("MultiRocketAmmo",1)
        TNT1 A 0 A_GiveInventory("Cell",60)
        stop
    }
}

actor HexenArmor1 : BasicArmorBonus replaces AmuletOfWarding
{
    RenderStyle Add
    Alpha 0.875
    +Floatbob
    Armor.Savepercent 75
    Armor.MaxSaveAmount 250
    Armor.Saveamount 25
    Inventory.PickupMessage "Picked up 25 Bonus Armor."
    Inventory.PickupSound "armor1get"
    States
    {
      Spawn:
        RMR4 A 0
        RMR4 A -1 bright ACS_ExecuteAlways(212, 0, 4, 100, 90)
        loop
    }
}

actor ArmorHexenTrail
{
    Renderstyle Add
    Alpha 0.5
    Scale 0.95
    -Solid
    +Noclip
    +Nogravity
    +Nointeraction
    +CLIENTSIDEONLY
    States
    {
      Spawn:
        RMR4 A 1 A_FadeOut(0.05)
        Loop
    }
}

actor HexenArmor5 : CustomInventory replaces ArtiBoostArmor
{
    RenderStyle Add
    Alpha 0.875
    +Floatbob
    Inventory.PickupMessage "Picked up 50 Extra Armor."
    Inventory.PickupSound "nosoundforyoubitch"
    States
    {
      Spawn:
        G_AM A 1 Bright
        Loop

      Pickup:
        TNT1 A 0 A_JumpIfInventory("MarathonClass",1,"PickupMarathon")
        TNT1 A 0 A_PlaySound("FighterHammerHitWall")
        TNT1 A 1 A_GiveInventory("HexenArmor5B")
        stop

      PickupMarathon:
        TNT1 A 0 A_SetBlend("White",0.5,5)
        TNT1 A 0 A_PlaySound("FighterHammerHitWall")
        TNT1 A 1 A_GiveInventory("HexenArmor5B")
        stop
    }
}

