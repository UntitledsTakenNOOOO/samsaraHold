actor HereticAmmoBackpack : AmmoBackpack replaces BagOfHolding { }

actor HereticStuffDump: HealPack0 replaces GoldWandHefty
{
    Scale 0.3
    Inventory.Amount 5
    Inventory.PickupMessage "Picked up 5 Health."
    States
    {
      Spawn:
        MEDI V -1 BRIGHT
        stop
        
      Pickup:
        TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(SAMSARA_DECORATE, 22), "Infinite")
        TNT1 A 0 A_GiveInventory("FiniteHealth200", 5)
        stop

      Infinite:
        TNT1 A 0 A_GiveInventory("InfiniteHealth", 5)
        stop
    }
}

