        QLTG F 2 A_WeaponReady
        QLTG GH 1 A_WeaponReady
        goto Ready

      Discharge:
        QLTG A 0 A_StopSound(6)
        QLTG A 0 A_StopSound(7)
        TNT1 A 0 A_JumpIfInventory("QuakeInvisTimer", 1, 2)
        TNT1 A 0 A_AlertMonsters
