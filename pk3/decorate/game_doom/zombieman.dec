actor Clip2: CustomInventory
{
    Inventory.PickupMessage "Picked up 5 rounds of Ammo 1."
    Inventory.PickupSound "ammo/bulletsmall"
    States
    {
      Spawn:
        CLIP B 3000
        stop

      Pickup:
        TNT1 A 0 A_JumpIfInventory("MarathonClass",1,"PickupMarathon")
        TNT1 A 0 A_JumpIfInventory("Clip",0,"PickupFail")
        TNT1 A 0 A_GiveInventory("Clip",5)
        stop

      PickupFail:
        CLIP B 1
        fail

      PickupMarathon:
        TNT1 A 0 A_JumpIfInventory("Clip",0,"PickupFail")
        TNT1 A 0 A_SetBlend("Green",0.4,4)
        TNT1 A 1 A_GiveInventory("Clip",10)
        stop
    }
}

actor Zombieman2 : Zombieman replaces ZombiemanStun
{
    DropItem "Clip2"
	painchance "Stunner", 255
    DamageFactor "CorvusCrossbow", 1.1
    DamageFactor "DragonClaw", 1.5
    PainChance "MightyBoot", 255
    BloodType "SamsaraBloodyHellBlood"
    +THRUGHOST
    +NOTDMATCH
    +NOICEDEATH
    states
    {
	Pain.Stunner:
		POSS G 3 A_ChangeFlag("NoPain", 1)
		POSS G 300 A_Pain
		POSS G 0 A_ChangeFlag("NoPain", 0)
		goto See
      Death.DukeDamage:

      Death.DukePistol:
        TNT1 A 0 A_Jump(218,"Death")
        TNT1 A 0 A_GiveToTarget("DukeKillTaunt",1)
        goto Death

      Death.MightyBoot:
        TNT1 A 0 A_PlaySoundEx("duke/kickhit","body",0,0)
        TNT1 A 0 A_Jump(64,"Death")
        TNT1 A 0 A_GiveToTarget("DukeBootTaunt",1)
        goto Death

      Death:
        TNT1 A 0 A_GiveToTarget("KillCount",1)
        goto "Super::Death"

      Death.DukeExplosive:
      Death.DukeSpectral:
        TNT1 A 0 A_JumpIfHealthLower(-3,"DukeGibStart")
        goto Death.DukeDamage

      DukeGibStart:
        POSS G 1
        TNT1 A 0 A_UnsetSolid
        TNT1 A 0 A_PlaySound("duke/enemygib")
        TNT1 A 0 A_SpawnDebris("DukeGibs1")
        TNT1 A 0 A_SpawnDebris("DukeGibs2")
        TNT1 A 0 A_SpawnDebris("DukeGibs3")
        TNT1 A 0 A_SpawnDebris("DukeGibs4")
        TNT1 A 0 A_SpawnDebris("DukeGibs5")
        TNT1 A 0 A_SpawnDebris("DukeGibs6")
        TNT1 A 0 A_Jump(96,"XDeath")
        TNT1 A 0 A_GiveToTarget("DukeGibTaunt",1)

	Death:
        	TNT1 A 0 A_GiveToTarget("KillCount",1)
		TNT1 A 0 A_GiveToTarget("AwardItem", 1)
		POSS H 5
		POSS I 5 A_Scream
		POSS J 5 A_NoBlocking
		POSS K 5
		POSS L 3000
		POSS LLLLLLLLLLLL 2 A_FadeOut(0.1)
		stop
	XDeath:
        	TNT1 A 0 A_GiveToTarget("KillCount",1)
		TNT1 A 0 A_GiveToTarget("AwardItem", 3)
		TNT1 A 0 ACS_ExecuteAlways(680,0,1)
		POSS M 5
		POSS N 5 A_XScream
		POSS O 5 A_NoBlocking
		POSS PQRST 5
		POSS U 3000
		POSS UUUUUUUUUUUU 2 A_FadeOut(0.1)
		stop

       XDeathNashgore:
         TNT1 A 0
         TNT1 A 0 A_SpawnItemEx("NashGore_Gib9",0,0,0,random(-350,350)/100.00,random(-350,350)/100.00,random(500,1000)/100.00,random(-180,180),33) //A_SpawnDebris("NashGore_Gib9",0)
         TNT1 A 0 A_SpawnItemEx("NashGore_Gib10",0,0,0,random(-350,350)/100.00,random(-350,350)/100.00,random(500,1000)/100.00,random(-180,180),33) //A_SpawnDebris("NashGore_Gib10",0)
         TNT1 AA 0 A_SpawnItemEx("NashGore_Gib11",0,0,0,random(-350,350)/100.00,random(-350,350)/100.00,random(500,1000)/100.00,random(-180,180),33) //A_SpawnDebris("NashGore_Gib11",0)
         TNT1 AA 0 A_SpawnItemEx("NashGore_GibGenerator",0,0,0,0,0,0,0,33)
        POSS A 5
         TNT1 AA 0 A_SpawnItemEx("NashGore_GibGenerator",0,0,0,0,0,0,0,33)
        POSS A 5 A_XScream
         TNT1 AA 0 A_SpawnItemEx("NashGore_GibGenerator",0,0,0,0,0,0,0,33)
        POSS A 5 A_NoBlocking
         TNT1 AA 0 A_SpawnItemEx("NashGore_GibGenerator",0,0,0,0,0,0,0,33)
	POSS U 3000
	POSS UUUUUUUUUUUU 2 A_FadeOut(0.1)
        stop

      Missile:
        POSS E 10 A_FaceTarget
        TNT1 A 0 A_PlaySound("grunt/attack")
        POSS F 8 A_CustomMissile("DoomEnemyBullet",32,0,random(-22.5,22.5),8)
        POSS E 8
        Goto See

      Pain.MightyBoot:
        TNT1 A 0 A_PlaySoundEx("duke/kickhit","body",0,0)
        POSS G 2 A_FaceTarget
        TNT1 A 0 A_Recoil(14)
        POSS G 16 A_Pain
        Goto See

      Death.ZorchPistol:

      Death.Zorch:
      Death.ZorchKamikaze:
      Death.PhasingZorch:
      Death.ZorchSpectral:
        TNT1 A 0 A_GiveToTarget("KillCount",1)
	TNT1 A 0 A_GiveToTarget("AwardItem", 1)
        POSS G 0 A_Stop
        TNT1 A 0 A_ChangeFlag("DONTGIB",1)
        TNT1 A 0 A_PlaySoundEx("chex/zorch","soundslot5",0,0)
        POSS G 3 A_SpawnItem("ZorchEffectSmall")
        TNT1 A 0 A_GiveInventory("EnemyZorched",1)
        POSS G 3 A_SetTranslucent(0.9,0)
        POSS G 0 A_Scream
        POSS G 3 A_SetTranslucent(0.8,0)
        POSS G 0 A_NoBlocking
        POSS G 3 A_SetTranslucent(0.7,0)
        POSS G 3 A_SetTranslucent(0.6,0)
        POSS G 3 A_SetTranslucent(0.5,0)
        POSS G 3 A_SetTranslucent(0.4,0)
        POSS G 3 A_SetTranslucent(0.3,0)
        POSS G 3 A_SetTranslucent(0.2,0)
        POSS G 3 A_SetTranslucent(0.1,0)
        TNT1 A 1 A_SetTranslucent(1,0)
        TNT1 A -1
        stop

      Raise:
        Stop

      ZorchRaise:
        TNT1 A 1 A_UnsetShootable
        TNT1 A 1 A_SpawnItem("SpawnFire2")
        TNT1 A 0 A_PlaySound("archvile/resummon")
        TNT1 A 1 A_TakeInventory("EnemyZorched",1)
        TNT1 A 0 A_SetTranslucent(0.1,0)
        POSS A 3 A_SetTranslucent(0.2,0)
        POSS A 3 A_SetTranslucent(0.3,0)
        POSS A 3 A_SetTranslucent(0.4,0)
        POSS A 3 A_SetTranslucent(0.5,0)
        POSS A 3 A_SetTranslucent(0.6,0)
        POSS A 3 A_SetTranslucent(0.7,0)
        POSS A 3 A_SetTranslucent(0.8,0)
        POSS A 3 A_SetTranslucent(0.9,0)
        POSS A 3 A_SetTranslucent(1,0)
        POSS A 1 A_SetShootable
        Goto See

      Death.DukeIce:
        TNT1 A 0 A_PlaySound("duke/enemyfreeze")
        TNT1 A 0 A_GiveInventory("FreezethrowerWeakness")
        TNT1 A 0 A_Jump(218,"Ice")
        TNT1 A 0 A_GiveToTarget("DukeKillTaunt",1)

      Ice:
      Death.HexenIce:

      Death.Ice:
        TNT1 A 0 A_GiveToTarget("KillCount",1)
	TNT1 A 0 A_GiveToTarget("AwardItem", 2)
        TNT1 A 0 A_GiveInventory("EnemyZorched",1)
        POSS G 0 A_GenericFreezeDeath
        POSS G 70 A_UnsetSolid
        POSS G 0 A_IceGuyDie
        POSS G 1 A_IceGuyDie
        TNT1 A -1
        stop
    }
}

