actor RedMistTrail
{
    Alpha 0.2
    Gravity 0.05
    Scale 0.35
    +NOBLOCKMAP
    +DONTSPLASH
    -EXPLODEONWATER
    -ALLOWPARTICLES
    -NOGRAVITY
    -SOLID
    +CLIENTSIDEONLY
        +NOINTERACTION
    States
    {
    Spawn:
         TNT1 A 0
                TNT1 A 0 A_Jump(128,"Spawn2")
        RMST AAAAABBBBBCCCCCDDDDDEEEEEFFFFFGGGGGHHHHH 1 A_FadeOut(0.005) // 40
                stop
        Spawn2:
        RMS_ AAAABBBBBCCCCDDDDDEEEEFFFFFGGGGHHHHHIIII 1 A_FadeOut(0.005) // 40
                stop
    }
}

