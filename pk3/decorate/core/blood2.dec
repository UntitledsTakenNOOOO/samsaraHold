actor RedMistTrail
{
    Alpha 0.2
    Gravity 0.05
    Scale 0.35
    +NOBLOCKMAP
    +DONTSPLASH
    -EXPLODEONWATER
    -ALLOWPARTICLES
    -NOGRAVITY
    -SOLID
    +CLIENTSIDEONLY
        +NOINTERACTION
    States
    {
    Spawn:
        RMST AAAAABBBBBCCCCCDDDDDEEEEEFFFFFGGGGGHHHHH 1 A_FadeOut(0.005) // 40
                stop
    }
}

