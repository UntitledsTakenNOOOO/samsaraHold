actor RedMist
{
    Speed 9
    Gravity 0.5
    Projectile
    +MISSILE
    +RANDOMIZE
    -NOGRAVITY
    States
        {
        Spawn:
         TNT1 A 0
         TNT1 A 2 A_SpawnItemEx("RedMistTrail",0,0,0,0,0,0,0,161)
         loop
    XDeath:
    Death:
        TNT1 A 1
        stop
        }
}

