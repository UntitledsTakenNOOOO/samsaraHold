actor CorvusPlayer : PlayerPawn
{
    Player.DisplayName "Corvus"
    Player.StartItem "Elven Wand", 1
    Player.StartItem " Staff ", 1
    Player.StartItem "CorvusClass", 1
    Player.StartItem "IsSamsaraClass", 1
    Player.StartItem "Clip", 50
    Player.StartItem "PermaTimeBomb", 1
    Player.StartItem "PermaRemoteBomb", 1
    Player.StartItem "PermaProxyBomb", 1
    Player.Face "CRV"
    Player.Soundclass "CorvusPlayer"
    Player.JumpZ 9//Default is 8
    BloodColor "Red"
    BloodType "SamsaraBloodyHellBlood"
	damagefactor "MarineAlly", 0
    DamageFactor "TimonsAxe", 0.75
    DamageFactor "PoisonCloud", 1.75
    //DamageFactor "Wraithverge", 0.75
    DamageFactor "CorvusCrossbow", 1.1
    DamageFactor "DragonClaw", 1.1
    DamageFactor "MightyBoot", 1.5
    DamageFactor "WMCBeam", 0.75
    DamageFactor "WMCNade", 0.75
    DamageFactor "EnemyInfightDamage", 0.0
    DamageFactor "Kamikaze", 0
    DamageFactor "ZorchKamikaze", 0
	Health 160
	Player.StartItem "LifeItem", 5    // starting lives
	Player.StartItem "CoinItem", 1500 // starting money
	Player.StartItem "GreenArmorModeStartGiver", 1 //starting armor
    +NOSKIN
    +DONTBLAST
    +THRUSPECIES
    Species "Player"
    Player.ColorRange 192, 207
    
    States
    {
      Spawn:
        CORV A -1
        Stop

      See:
        CORV ABCD 4
        Loop

      Melee:
      Missile:
        CORV F 6 BRIGHT
        CORV E 12
        Goto Spawn

      Pain:
        TNT1 A 0 A_JumpIfInventory("WolfExtraLife",1,"GoingToSalvate")
        CORV G 4
        CORV G 4 A_Pain
        Goto Spawn

      GoingToSalvate:
        CORV G 4 A_JumpIfHealthLower(5, "GoingTo")
        CORV G 4 A_Pain
        Goto Spawn

      GoingTo:
        TNT1 A 0 A_GiveInventory("Medikit")
        TNT1 A 0 A_GiveInventory("Medikit")
        TNT1 A 0 A_GiveInventory("Medikit")
        TNT1 A 0 A_GiveInventory("Medikit")
        TNT1 A 0 A_GiveInventory("Salvate")
        TNT1 A 0 A_GiveInventory("WolfLifeProtection")
        TNT1 A 0 A_PlayerScream
        CORV G 4 A_TakeInventory("WolfExtraLife",1)
        CORV G 4
        Goto Spawn

      Death:
	TNT1 A 0 ACS_ExecuteAlways(SAMSARA_CLIENT_DECORATE,0,11)
      Death2:
        CORV H 6
        CORV I 6 A_PlayerScream
        CORV JK 6
        CORV L 6 A_NoBlocking
        CORV MNO 6
        CORV P -1
        Stop

      XDeath:
	TNT1 A 0 ACS_ExecuteAlways(SAMSARA_CLIENT_DECORATE,0,10)
      XDeath2:
        TNT1 A 0 A_SpawnItemEx("ZombiemanNashgoreXDeath",0,0,0,0,0,0,0,33)
        CORV Q 5 A_PlayerScream
        TNT1 A 0 A_SpawnItemEx("BloodySkull2",0,0,46,random(-1,1),random(-1,1),5)
        CORV R 5 A_NoBlocking
        CORV STUVWX 5
        CORV Y -1
        Stop

      Death.CorvusGrimBallista:
        CORV Q 5 A_PlaySoundEx("*falling",CHAN_VOICE,0,0)
        TNT1 A 0 A_SpawnItemEx("BloodySkull2",0,0,46,random(-1,1),random(-1,1),5)
        CORV R 5 A_NoBlocking
        CORV STUVWX 5
        CORV Y -1
        Stop
        
      Death.Inferno:
        //Death.MarathonPlasma:
      Death.HexGreenFire:
      Death.QuakeFire:
	  Death.FireExplosive:
      Burn:
        FDTH A 5 BRIGHT A_PlaySound("*burndeath")
        FDTH B 4 BRIGHT
        FDTH C 5 BRIGHT
        FDTH D 4 BRIGHT
        FDTH E 5 BRIGHT
        FDTH F 4 BRIGHT
        FDTH G 5 BRIGHT
        FDTH H 4 BRIGHT
        FDTH I 5 BRIGHT
        FDTH J 4 BRIGHT
        FDTH K 5 BRIGHT
        FDTH L 4 BRIGHT
        FDTH M 5 BRIGHT
        FDTH N 4 BRIGHT
        FDTH O 5 BRIGHT A_NoBlocking
        FDTH P 4 BRIGHT
        FDTH Q 5 BRIGHT
        FDTH R 4 BRIGHT
        ACLO E -1
        stop

      Death.ZorchPistol:

      Death.Zorch:
      Death.ZorchKamikaze:
      Death.PhasingZorch:
      Death.ZorchSpectral:
        CORV G 0 A_Stop
        TNT1 A 0 A_PlaySoundEx("chex/zorch","soundslot5",0,0)
        CORV G 3 A_SpawnItem("ZorchEffectSmall")
        CORV G 3 A_SetTranslucent(0.9,0)
        CORV G 0 A_PlayerScream
        CORV G 3 A_SetTranslucent(0.8,0)
        CORV G 0 A_NoBlocking
        CORV G 3 A_SetTranslucent(0.7,0)
        CORV G 3 A_SetTranslucent(0.6,0)
        CORV G 3 A_SetTranslucent(0.5,0)
        CORV G 3 A_SetTranslucent(0.4,0)
        CORV G 3 A_SetTranslucent(0.3,0)
        CORV G 3 A_SetTranslucent(0.2,0)
        CORV G 3 A_SetTranslucent(0.1,0)
        ACLO E 1 A_SetTranslucent(1,0)
        ACLO E -1
        stop

      CoOpModeOn:
        TNT1 A 0
        TNT1 A 0 A_ChangeFlag("THRUSPECIES",1)
        TNT1 A 0 A_ChangeFlag("DONTBLAST",1)
        CORV A 1
        goto Spawn

      Death.DukeDamage:

      Death.DukePistol:
        TNT1 A 0 A_Jump(218,"Death")
        TNT1 A 0 A_GiveToTarget("DukeDMTaunt1",1)
        goto Death

      Death.DukeExplosive:
      Death.DukeSpectral:
        TNT1 A 0 A_JumpIfHealthLower(-5,"DukeGibStart")
        goto Death.DukeDamage

      DukeGibStart:
	CORV G 1
        TNT1 A 0 A_UnsetSolid
        TNT1 A 0 A_PlaySound("duke/enemygib")
        TNT1 A 0 A_SpawnDebris("DukeGibs1")
        TNT1 A 0 A_SpawnDebris("DukeGibs2")
        TNT1 A 0 A_SpawnDebris("DukeGibs3")
        TNT1 A 0 A_SpawnDebris("DukeGibs4")
        TNT1 A 0 A_SpawnDebris("DukeGibs5")
        TNT1 A 0 A_SpawnDebris("DukeGibs6")
        TNT1 A 0 A_Jump(96,"XDeath")
        TNT1 A 0 A_GiveToTarget("DukeDMTaunt1",1)
        goto XDeath

      Death.DukeIce:
        TNT1 A 0 A_Jump(218,"Ice")
        TNT1 A 0 A_GiveToTarget("DukeDMTaunt1",1)
        goto Ice

      Ice:
      Death.HexenIce:

      Death.Ice:
	TNT1 A 0 A_PlayWeaponSound("misc/freeze")
        CORV Z 70 A_NoBlocking//A_GenericFreezeDeath
        CORV Z 0 A_SetTranslucent(0,0)
        TNT1 AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA 0 A_SpawnItemEx("IceChunk2",random(-1,1),random(-1,1),random(8,48),random(-2,2),random(-2,2),random(1,6))
        CORV Z 1 A_PlayWeaponSound("misc/icebreak")//A_FreezeDeathChunks
        CORV Z -1
        stop
        //CORV G 5 A_GenericFreezeDeath
        //CORV G 1 A_FreezeDeathChunks
        //Wait

      Pain.MightyBoot:
	    CORV G 1 ACS_ExecuteAlways(312, 0, 25, 5, 24)
        CORV G 1 A_PlaySoundEx("duke/kickhit","body",0,0)
        TNT1 A 0 A_Recoil(12)
        CORV G 6 A_Pain
        Goto See
		
	  Pain.MarathonStun:
        TNT1 A 0 A_Stop
	    CORV G 1 ACS_ExecuteAlways(312, 0, 25, 5, 100)
		TNT1 A 0 A_GiveInventory("NothingSpeed")
		TNT1 A 0 A_SetBlend("White",0.9,105)
        CORV G 1 A_PlaySoundEx("marathon/tech50/stun","body",0,0)
        CORV G 6 A_Pain
        Goto See

        DeathBrutal:
         TNT1 A 0
                TNT1 AA 0 A_SpawnItemEx("RedMistTrail",random(-10,10),random(-10,10),random(12,40),random(-400,400)/100.00,random(-400,400)/100.00,random(300,600)/100.00,random(-180,180),160)
                TNT1 AAA 0 A_SpawnItemEx("BrutalTinyBone",random(-5,5),random(-5,5),random(12,40),random(-200,200)/100.00,random(-200,200)/100.00,random(300,800)/100.00,random(-180,180),160)
