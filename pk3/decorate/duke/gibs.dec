actor DukeGibs1
{
    Radius 1
    Height 1
    Speed 0
    Projectile
    -NOGRAVITY
    -SOLID
    +MOVEWITHSECTOR
    +CLIENTSIDEONLY
    +NOBLOCKMAP
    +NOTELEPORT
    Gravity 0.65
    Health 2
    scale 0.5
    States
    {
      Spawn:
        DKG1 ABCD 5
        loop

      Death:
        TNT1 A 0 A_Gravity
        DKG1 E 1000
	TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(SAMSARA_DECORATE,31) == 1, "Death")
        DKG1 E 50
        stop
        stop
    }
}

